  "keywords": [
    "aws",
    "lambda",
    "dynamodb",
    "items",
    "hostory",
    "transactions"
  ],
  "author": "akrsmv",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^13.13.15",
    "aarts-dynamodb": "^0.4.23",
    "aarts-eb-dispatcher": "^1.0.16",
    "aarts-eb-dispatcher-tester": "^1.0.17",
    "aarts-eb-handler": "^0.3.16",
    "aarts-eb-notifier": "0.0.7",
    "aarts-eb-types": "^0.2.29",
    "typescript": "^3.9.7"
  },
  "devDependencies": {}
}
