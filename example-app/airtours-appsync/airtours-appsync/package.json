{
  "name": "touristart",
  "version": "0.0.16",
  "description": "example business domain using aws lambda and dynamodb via the aarts framework ",
  "main": "index.js",
  "scripts": {
    "build_win": "npm run clean_win && tsc && copy package.json .\\dist",
    "build_lin": "npm run clean_lin && tsc && cp package.json ./dist",
