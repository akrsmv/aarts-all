    "build": "tsc && cp package.json aarts.sh ln-cross-platform.sh env-constants-local.json ./dist && echo {\\\"app\\\": \\\"node bin/aarts-cdk-typescript.js\\\"} > ./dist/cdk.json",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
