{
  "name": "aarts-cli",
  "version": "0.1.910",
  "bin": {
    "aarts": "aarts.sh",
    "aarts-cdk-typescript": "bin/aarts-cdk-typescript.js"
  },
  "main": "./dist",
  "files": [
    "bin/**",
    "lib/**",
    "*.json",
    "*.sh",
    "*.js",
    "*.d.ts",
